{
          "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
            "width": 800,
            "height": 600,
            "projection": {
                "type": "mercator",
                "center": [135, -28],
                "scale": 890 
            },
            "layer": [
                {
                    "data": {
                        "url": "https://raw.githubusercontent.com/vega/vega-datasets/master/data/world-110m.json",
                        "format": {
                            "type": "topojson",
                            "feature": "countries"
                        }
                    },
                    "mark": {
                        "type": "geoshape",
                        "fill": "lightgray",
                        "stroke": "white"
                    }
                },
                {
                    "data": {
                        "url": "https://raw.githubusercontent.com/FIT3179/Vega-Lite/main/2_symbol_map/js/WorldMapWithGraticules.topojson",
                        "format": {"type": "topojson", "feature": "ne_110m_graticules_30"}
                    },
                    "mark": {"type": "geoshape", "fill": null, "stroke": "lightgray"}
                },
                {
                    "data": {
                        "url": "https://raw.githubusercontent.com/FIT3179/Vega-Lite/main/7_others/oceans.topojson",
                        "format": {"type": "topojson", "feature": "oceans"}
                    },
                    "mark": {"type": "geoshape", "fill": "skyblue"}
                },
                {
                    "data": {
                        "url": "https://raw.githubusercontent.com/gcha0035/FIT3179HW-week10/main/Wine_Geographical_Indications_Australia_2023.topojson",
                        "format": {
                            "type": "topojson",
                            "feature": "Wine_Geographical_Indications_Australia_2023"
                        }
                    },
                    "mark": {
                        "type": "geoshape",
                        "fill": "#ccc",
                        "stroke": "black"
                    }
                },
                {
                    "data": {
                        "url": "https://raw.githubusercontent.com/gcha0035/FIT3179HW-week10/main/Wine_Geographical_Indications_Australia_2023%20(1).topojson",
                        "format": {
                            "type": "topojson",
                            "feature": "Wine_Geographical_Indications_Australia_2023 (1)"
                        }
                    },
                    "mark": {
                        "type": "geoshape",
                        "fill": "none",
                        "stroke": "white"
                    }
                },
                {
                    "data": {
                        "url": "https://raw.githubusercontent.com/gcha0035/FIT3179HW-week10/main/Wine_Geographical_Indications_Australia_2023%20(2).topojson",
                        "format": {
                            "type": "topojson",
                            "feature": "Wine_Geographical_Indications_Australia_2023 (2)"
                        }
                    },
                    "mark": {
                        "type": "geoshape",
                        "stroke": "lightgray" 
                    },
                    "encoding": {
                        "color": {
                            "field": "properties.STATE", 
                            "type": "nominal",
                            "scale": {
                                "domain": ["NSW", "QLD", "SA", "TAS", "VIC", "WA"],
                                "range": [
                                    "#4B0082", 
                                    "#9370DB", 
                                    "#755a7d", 
                                    "#755a7d", 
                                    "#ba4984", 
                                    "#9B1B30" 
                                ]
                            },
                            "title": "State"
                        },
                        "tooltip": [
                            {"field": "properties.GI_NAME", "type": "nominal", "title": "Region Name"}
                        ]
                    }
                }

            ]
        }